<?xml version="1.0" encoding="utf-8"?>
<Booklet xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/iqb-berlin/testcenter-backend/12.2.0/definitions/vo_Booklet.xsd">
  <Metadata>
    <Id>BOOKLET.SAMPLE-3</Id>
    <Label>Similar Booklet to Sample 1</Label>
    <Description>
      This booklet has the same species as sample Booklet 1. The term booklet species indicates that both booklets can
      be controlled by the group-monitor as if they where the same.
      The species is currently defined as having the same number of testlets on the first level (called blocks). This
      definition may change later.
    </Description>
  </Metadata>

  <Units>

    <Testlet id="another_testlet" label="First Block">
      <Unit id="UNIT.SAMPLE" label="Sample Unit" labelshort="Sample Unit" />
    </Testlet>

    <Testlet id="a_testlet" label="2nd Block here, 1st Block in >BOOKLET.SAMPLE-1">
      <Unit id="UNIT.SAMPLE-2" label="A very Simple Sample Unit" labelshort="2nd Sample Unit" />

      <Testlet id="sub_testlet" label="A Subtestlet">
        <Unit id="UNIT.SAMPLE-3" label="Sample Unit again, with Alias" labelshort="Sample Unit Again" alias="an_alias" />
      </Testlet>
    </Testlet>

  </Units>
</Booklet>
